@model IEnumerable<WakeMe.Models.WakeOnLanEntry>
@{
    ViewBag.Title = "Home Page";
}

<table class="table table-hover table-condensed">
    <thead>
        <tr>
            <th>Name</th>
            <th>Mac Address</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var i in Model)
        {
            <tr>
                <td>@i.Name</td>
                <td>@i.MacAddress</td>
                <td>
                    @Html.Partial("WakeOnLanEntry", i)
                </td>
            </tr>
        }
    </tbody>
</table>